{
    "version": 2,
    "builds": [
      { "src": "Frontend/Build", "use": "@vercel/static-build" },
      { "src": "Servers/index.cjs", "use": "@vercel/node" }
    ],
    "routes": [
      { "src": "/api/.*", "dest": "Servers/index.cjs" },
      { "src": "/(.*)", "dest": "Frontend/public/index.html" }
    ]
  }
  