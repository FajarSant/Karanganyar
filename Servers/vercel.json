{
  "version": 2,
  "builds": [
    {
      "src": "Servers/HotNews.cjs",
      "use": "@vercel/node"
    },
    {
      "src": "Servers/ListNews.cjs",
      "use": "@vercel/node"
    },
    {
      "src": "Servers/Berita.cjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/hotnews", "dest": "Servers/HotNews" },
    { "src": "/api/listnews", "dest": "Servers/ListNews" },
    { "src": "/api/Berita", "dest": "Servers/Berita" },
    { "src": "/(.*)", "dest": "Servers/index" }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
