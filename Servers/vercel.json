{
  "version": 2,
  "builds": [
  ],
  "routes": [
    { "src": "/api/hotnews", "dest": "Frontend/src/Pages/HotNews.js" },
    { "src": "/api/listnews", "dest": "Frontend/src/Pages/ListNews.cjs" },
    { "src": "/api/Berita", "dest": "Frontend/src/Pages/Berita.cjs" },
    { "src": "/(.*)", "dest": "Servers/index.js" }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
