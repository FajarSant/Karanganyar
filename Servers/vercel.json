{
  "version": 2,
  "builds": [
    { "src": "api/HotNews.cjs", "use": "@vercel/node" },
    { "src": "api/ListNews.cjs", "use": "@vercel/node" },
    { "src": "api/Berita.cjs", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/api/hotnews", "dest": "Servers/HotNews.cjs" },
    { "src": "/api/listnews", "dest": "Servers/ListNews.cjs" },
    { "src": "/api/berita", "dest": "Servers/Berita.cjs" },
    { "src": "/(.*)", "dest": "Servers/index.js" }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
